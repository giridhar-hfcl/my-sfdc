<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>valid_phone</fullName>
    <active>true</active>
    <errorConditionFormula>NOT(ISNUMBER(Phone__c))</errorConditionFormula>
    <errorDisplayField>Phone__c</errorDisplayField>
    <errorMessage>Phone should contain only numbers</errorMessage>
</ValidationRule>
